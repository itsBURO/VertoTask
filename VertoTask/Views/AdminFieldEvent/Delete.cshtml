@model VertoTask.Models.FieldEvent

<h2>Delete Field Event?</h2>
<p>Are you sure you want to delete this Field Event?</p>
<h3>@Model.EventName</h3>
<form asp-action="DeleteConfirmed" asp-controller="AdminFieldEvent" method="post">
    <input type="hidden" asp-for="Id" />
    <button type="submit" class="button alert">Delete</button>
    <a asp-action="Index" class="button">Cancel</a>
</form>
